{
  "resource_version": 8,
  "metadata": {
    "version": 3,
    "sources": [
      {
        "name": "default",
        "kind": "postgres",
        "tables": [
          {
            "table": {
              "name": "characters",
              "schema": "public"
            },
            "object_relationships": [
              {
                "name": "show",
                "using": {
                  "foreign_key_constraint_on": "show_id"
                }
              }
            ]
          },
          {
            "table": {
              "name": "episodes",
              "schema": "public"
            },
            "object_relationships": [
              {
                "name": "season",
                "using": {
                  "foreign_key_constraint_on": "season_id"
                }
              }
            ]
          },
          {
            "table": {
              "name": "seasons",
              "schema": "public"
            },
            "object_relationships": [
              {
                "name": "show",
                "using": {
                  "foreign_key_constraint_on": "show_id"
                }
              }
            ],
            "array_relationships": [
              {
                "name": "episodes",
                "using": {
                  "foreign_key_constraint_on": {
                    "column": "season_id",
                    "table": {
                      "name": "episodes",
                      "schema": "public"
                    }
                  }
                }
              }
            ]
          },
          {
            "table": {
              "name": "sessions",
              "schema": "public"
            }
          },
          {
            "table": {
              "name": "shows",
              "schema": "public"
            },
            "array_relationships": [
              {
                "name": "characters",
                "using": {
                  "foreign_key_constraint_on": {
                    "column": "show_id",
                    "table": {
                      "name": "characters",
                      "schema": "public"
                    }
                  }
                }
              },
              {
                "name": "seasons",
                "using": {
                  "foreign_key_constraint_on": {
                    "column": "show_id",
                    "table": {
                      "name": "seasons",
                      "schema": "public"
                    }
                  }
                }
              }
            ]
          }
        ],
        "configuration": {
          "connection_info": {
            "database_url": {
              "from_env": "HASURA_GRAPHQL_DATABASE_URL"
            },
            "isolation_level": "read-committed",
            "pool_settings": {
              "connection_lifetime": 600,
              "idle_timeout": 180,
              "max_connections": 50,
              "retries": 1
            },
            "use_prepared_statements": true
          }
        }
      }
    ]
  }
}