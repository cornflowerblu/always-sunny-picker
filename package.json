{
  "name": "always-sunny-picker",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "node ./bin/www",
    "dev": "npm run build && npm start",
    "dev:live": "tsc-watch --onSuccess 'npm run dev'",
    "test": "docker compose up -d && npx cypress run --browser chrome && rm -rf ./cypress/videos",
    "test:ui": "docker compose up -d && npm run prune:js && npx cypress open",
    "prune:js": "rm -rf **/*.js && rm -rf cypress/**/*.js && rm -f cypress.config.js"
  },
  "dependencies": {
    "@clerk/clerk-js": "^4.9.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.2",
    "express": "~4.16.1",
    "graphql-request": "^5.0.0",
    "http-errors": "~1.6.3",
    "ioredis": "^5.2.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "tiny-invariant": "^1.2.0",
    "url": "^0.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.18",
    "@types/uuid": "^8.3.4",
    "cypress": "^10.9.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.3"
  }
}
